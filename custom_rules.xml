<?xml version="1.0" encoding="UTF-8"?><project name="custom_rules">

  <target name="-pre-build">
    <exec executable="${ndk.dir}/ndk-build.cmd" failonerror="true"/>
  </target>
  
  
  <target depends="android_rules.clean" name="clean">
    <exec executable="${ndk.dir}/ndk-build.cmd" failonerror="true">
      <arg value="clean"/>
    </exec>
  </target>
  

</project>
